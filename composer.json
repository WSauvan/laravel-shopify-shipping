{
    "name": "webqamdev/laravel-shopify-shipping",
    "description": "A Laravel package to manage Shopify custom shipping rates.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sauvan William",
            "email": "william.sauvan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ShopifyShipping\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShopifyShipping\\ShopifyShippingServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
